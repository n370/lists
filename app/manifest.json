{
  "manifest_version": 2,
  "name": "Lists Enhancements for Twitter",
  "version": "1.0.0",

  "description": "Makes lists more accessible in Twitter.",
  "author": "tomitm",

  "icons": { "128": "icon128.png" },

  "permissions": [
    "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "contentscript.js"
      ],
      "css": [
        "main.css"
      ]
    }
  ]
}
